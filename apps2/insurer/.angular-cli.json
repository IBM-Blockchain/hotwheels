{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "insurer"
  },
  "apps": [
    {
      "root": "client/src",
      "outDir": "client/dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../../node_modules/leaflet/dist/leaflet.css",
        "styles.css"
      ],
      "scripts": [
        "../../node_modules/leaflet/dist/leaflet.js"
      ],
      "environmentSource": "client/environments/environment.ts",
      "environments": {
        "dev": "client/environments/environment.ts",
        "prod": "client/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "client/src/tsconfig.app.json"
    },
    {
      "project": "client/src/tsconfig.spec.json"
    },
    {
      "project": "client/e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
